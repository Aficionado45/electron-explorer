{
  "name": "electron-explorer",
  "version": "0.1.5",
  "private": true,
  "main": "public/main.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aficionado45/electron-explorer"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.10.2",
    "@electron/remote": "^1.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.27.2",
    "bootstrap": "^5.0.0-beta1",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^5.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "build:prod": "react-scripts build && electron-builder --win",
    "build:publish": "react-scripts build && electron-builder --win --publish always"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron",
    "productName": "Autoupdater Electron",
    "directories": {
      "output": "electron-build",
      "buildResources": "build-assets"
    },
    "win": {
      "publish": [
        "github"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "prettier": "^2.2.1",
    "wait-on": "^5.3.0"
  }
}
